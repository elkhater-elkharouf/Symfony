{% extends 'base-back.html.twig' %}
{% block body %}
    <h1>Modifier un réservation</h1>

    {{form_start(formA)}}


    <div class="form-group">
        <label for="">Date arrivée</label>
        {{ form_widget (formA.date_debut) }}
    </div>
    <div class="form-group">
        <label for="">Date depart</label>
        {{ form_widget (formA.date_fin) }}
    </div>
    <div class="form-group">
        <label for="">Prix </label>
        {{ form_widget (formA.prixreservation) }}
    </div>
    <div class="form-group">
        <label for="">Nombre de personne </label>
        {{ form_widget (formA.nbrperson) }}
    </div>

    <button type="submit" class="btn btn-success">Modifier réservation</button>

    {{form_end(formA)}}


{% endblock %}