<div id="booking" class="section">
    <div class="section-center">
        <div class="container">
            <div class="row">
                <div class="booking-form">
                    <div class="form-header">
                        <h1>Book a car</h1>
                    </div>
                    {{ form_start(formA) }}
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="form-group">

                                    <span class="form-label">Name</span>
                                    {{ form_widget(formA.user , {'attr':{'class' : 'form-control' }}) }}
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <span class="form-label">Email</span>
                                    <input class="form-control" type="email" placeholder="Enter your email">
                                </div>
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-sm-5">
                                <div class="form-group">
                                    <span class="form-label">Pick-up Date</span>
                                    {{ form_widget(formA.datedebut , {'attr':{'class' : 'date-input' }}) }}



                                </div>
                            </div>
                            <div class="col-sm-5">
                                <div class="form-group">
                                    <span class="form-label">Drop-off Date</span>
                                    {{ form_widget(formA.datefin , {'attr':{'class' : 'date-input' }}) }}


                                </div>
                            </div>
                        </div>
                        <div class="form-btn">
                            <button class="submit-btn">Book Now</button>
                        </div>

                </div>
            </div>
        </div>
    </div>
</div>
