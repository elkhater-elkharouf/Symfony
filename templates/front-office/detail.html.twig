{% extends 'base-front.html.twig' %}
{% block image %}   url('/front-office/images/im3.jpg'); {% endblock %}
{% block img %} Car Details {% endblock %}
{% block body %}

    <link href="https://fonts.googleapis.com/css?family=Poppins:200,300,400,500,600,700,800&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="{{asset('front-office/css/voiture/open-iconic-bootstrap.min.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/animate.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/owl.carousel.min.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/owl.theme.default.min.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/magnific-popup.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/aos.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/ionicons.min.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/bootstrap-datepicker.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/jquery.timepicker.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/flaticon.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/icomoon.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/voiture/style.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/css/style.css')}}" type="text/css">






    <section class="ftco-section ftco-car-details">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-12">
                    <div class="car-details">
                        <div class="img rounded" style="background-image: url('/uploads/{{ voiture.image }}');"></div>
                        <div class="text text-center">
                            <span class="subheading">{{ voiture.marque }}</span>
                            <h2>{{ voiture.modele }} {{ voiture.couleur }}</h2>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md d-flex align-self-stretch ftco-animate">
                    <div class="media block-6 services">
                        <div class="media-body py-md-4">
                            <div class="d-flex mb-3 align-items-center">
                                <div class="icon d-flex align-items-center justify-content-center"><span class="flaticon-dashboard"></span></div>
                                <div class="text">
                                    <h3 class="heading mb-0 pl-3">
                                        Mileage
                                        <span>{{ voiture.killometrage }} km</span>
                                    </h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md d-flex align-self-stretch ftco-animate">
                    <div class="media block-6 services">
                        <div class="media-body py-md-4">
                            <div class="d-flex mb-3 align-items-center">
                                <div class="icon d-flex align-items-center justify-content-center"><span class="flaticon-pistons"></span></div>
                                <div class="text">
                                    <h3 class="heading mb-0 pl-3">
                                        Transmission
                                        <span>{{ voiture.transmission }}</span>
                                    </h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md d-flex align-self-stretch ftco-animate">
                    <div class="media block-6 services">
                        <div class="media-body py-md-4">
                            <div class="d-flex mb-3 align-items-center">
                                <div class="icon d-flex align-items-center justify-content-center"><span class="flaticon-car-seat"></span></div>
                                <div class="text">
                                    <h3 class="heading mb-0 pl-3">
                                        Seats
                                        <span>{{ voiture.place }} Adults</span>
                                    </h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md d-flex align-self-stretch ftco-animate">
                    <div class="media block-6 services">
                        <div class="media-body py-md-4">
                            <div class="d-flex mb-3 align-items-center">
                                <div class="icon d-flex align-items-center justify-content-center"><span class="flaticon-backpack"></span></div>
                                <div class="text">
                                    <h3 class="heading mb-0 pl-3">
                                        Luggage
                                        <span>{{ voiture.bagage }} Bags</span>
                                    </h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md d-flex align-self-stretch ftco-animate">
                    <div class="media block-6 services">
                        <div class="media-body py-md-4">
                            <div class="d-flex mb-3 align-items-center">
                                <div class="icon d-flex align-items-center justify-content-center"><span class="flaticon-diesel"></span></div>
                                <div class="text">
                                    <h3 class="heading mb-0 pl-3">
                                        Fuel
                                        <span>{{ voiture.carburant }}</span>
                                    </h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>


    <script src="{{asset('front-office/js/voiture/jquery.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/jquery-migrate-3.0.1.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/popper.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/bootstrap.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/jquery.easing.1.3.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/jquery.waypoints.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/jquery.stellar.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/owl.carousel.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/jquery.magnific-popup.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/aos.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/jquery.animateNumber.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/bootstrap-datepicker.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/jquery.timepicker.min.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/scrollax.min.js')}}"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="{{asset('front-office/js/voiture/google-map.js')}}"></script>
    <script src="{{asset('front-office/js/voiture/main.js')}}"></script>
{% endblock %}